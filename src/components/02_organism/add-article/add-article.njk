<form class="add-article" method="POST">
    <fieldset class="add-article__section">
        <legend class="add-article__title h2">{{ data.title }}</legend>

        {% for input in data.inputs %}
            {% molecule 'label-input', input %}
        {% endfor %}

        <div class="add-article__submit">
            {% atom 'button', 'button.json' %}
        </div>
    </fieldset>
</form>






